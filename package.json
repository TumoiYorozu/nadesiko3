{
  "name": "nadesiko3",
  "version": "0.1.6",
  "description": "Japanese Programming Language",
  "main": "src/index.js",
  "bin": {
    "cnako3": "src/cnako3.js"
  },
  "scripts": {
    "test": "mocha",
    "start": "lite-server",
    "electron": "electron src/enako3.js",
    "build": "webpack",
    "build:command": "node src/cnako3.js batch/build_command.nako3",
    "build:electron": "asar pack src/enako3.js release/enako3.asar",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kujirahand/nadesiko3.git"
  },
  "keywords": [
    "script"
  ],
  "author": "kujirahand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kujirahand/nadesiko3/issues"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11",
              "last 1 version",
              "last 2 Safari versions",
              "last 2 iOS versions"
            ],
            "useBuiltIns": true
          }
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "mocha": true,
      "browser": true
    },
    "rules": {
      "no-console": "off"
    },
    "plugins": [
      "react"
    ]
  },
  "homepage": "https://github.com/kujirahand/nadesiko3#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "date-utils": "^1.2.21",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^4.2.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "lite-server": "^2.3.0",
    "mocha": "^3.5.3",
    "prop-types": "^15.5.8",
    "pump": "^1.0.2",
    "source-map": "^0.5.7",
    "uuid": "^3.1.0",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
